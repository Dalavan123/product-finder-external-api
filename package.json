{
  "name": "productfinder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:vite": "rimraf node_modules/.vite .vite-cache",
    "dev": "npm run clean:vite && vite",
    "api": "node ./src/server/index.js",
    "dev:all": "concurrently -n WEB,API -c green,blue \"npm:dev\" \"npm:api\"",
    "test": "vitest",
    "test:run": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cy:open": "cypress open",
    "e2e": "cypress run",
    "e2e:dev": "concurrently -n WEB,CYPRESS -c green,magenta \"npm run dev\" \"npm run cy:open\"",
    "e2e:preview": "concurrently -n WEB,CYPRESS -c green,magenta \"npm run preview\" \"npm run cy:open\"",
    "cy:open:real": "cypress open --env USE_REAL_API=true",
    "e2e:real": "cypress run --env USE_REAL_API=true",
    "e2e:ci": "concurrently -n WEB,CYPRESS -c green,magenta \"vite preview --port 4173\" \"cypress run --config baseUrl=http://localhost:4173\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.0.0",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.9"
  }
}

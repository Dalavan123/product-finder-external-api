{
  "name": "productfinder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:vite": "rimraf node_modules/.vite .vite-cache",
    "dev": "npm run clean:vite && vite",
    "api": "node ./src/server/index.js",
    "dev:all": "concurrently -n WEB,API -c green,blue \"npm:dev\" \"npm:api\"",
    "test": "vitest",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "vite": "^7.1.2",
    "vitest": "^2.1.9"
  }
}
